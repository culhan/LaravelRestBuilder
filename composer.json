    "authors": [
        {
            "name": "A'mal Sholihan",
            "email": "amalsholihan@gmail.com",
            "homepage": "http://tryits.web.id"
        }
    ],
    "homepage": "https://github.com/culhan/laravelrestbuilder",
    "keywords": ["Laravel", "LaravelRestBuilder"],
    "require": {
        "illuminate/support": "~5",
        "doctrine/dbal": "v2.9.2",
        "khancode/laravelbaserest": "dev-master",
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KhanCode\\LaravelRestBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KhanCode\\LaravelRestBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KhanCode\\LaravelRestBuilder\\LaravelRestBuilderServiceProvider"
            ],
            "aliases": {
                "LaravelRestBuilder": "KhanCode\\LaravelRestBuilder\\Facades\\LaravelRestBuilder"
            }
        }
    }
}
